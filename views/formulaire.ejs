<%- include('header'); -%>
  <form action="/formulaire" method="post">
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="Prenom">Prénom</label>
        <input name="Prenom" type="text" pattern="[A-Za-z]+" class="form-control" id="Prenom" placeholder="Marcin" required>
      </div>
      <div class="form-group col-md-6">
        <label for="Nom">Nom</label>
        <input name="Nom" type="text" pattern="[A-Za-z]+" class="form-control" id="Nom" placeholder="Gutmanski" required>
      </div>
    </div>
    <div class="form-group">
      <label for="Email">Email</label>
      <input name="Email" type="email" class="form-control" id="Email" placeholder="guma0911@gmail.com" required>
    </div>
    <div class="form-group">
      <label for="Rue">Rue</label>
      <input name="Rue" type="text" class="form-control" id="Rue" placeholder="Rue du Postillon" required>
    </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="Numero">Numéro</label>
        <input name="Numero" type="number" class="form-control" id="Numero" placeholder="4" required>
      </div>
      <div class="form-group col-md-4">
        <label for="codePostal">Code postal</label>
        <input name="codePostal" type="number" id="codePostal" pattern="[0-9]{4}" class="form-control" placeholder="1180" required>
      </div>
      <div class="form-group col-md-2">
        <label for="Commune">Commune</label>
        <input name="Commune" type="text" class="form-control" pattern="[A-Za-z]+" id="Commune" placeholder="Uccle" required>
      </div>
    </div>
    <button type="submit" class="btn btn-primary">Envoyer</button>
  </form>
<%- include('footer') -%>
